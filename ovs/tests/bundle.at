OVS_VSWITCHD_START([dnl
    add-port br0 p1 -- set Interface p1 type=dummy -- \
    set Interface p1 ofport_request=1 -- \
    add-port br0 p2 -- set Interface p2 type=dummy -- \
    set Interface p2 ofport_request=2
])
